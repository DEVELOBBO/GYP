<mapper namespace="gymMapper">

<!-- 체육관 회원 데이터 불러오기 -->
<select id="getgymReadData" parameterType="hashMap" resultType="com.exe.dto.GymDTO"> 
	select gymId,gymName,gymPwd,gymEmail,gymTel,gymAddr,gymCreated,gymPass from GYM where gymId=#{sessionId}
</select>


